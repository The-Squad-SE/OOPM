import java.util.*;
class binary
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int a[]=new int[20];

System.out.println("enter the no to be converted");
int n=sc.nextInt();
int i=0,rem;
while(n!=0)
{
rem=n%2;
a[i]=rem;
n=n/2;
i++;
}
System.out.println("the obtained binary no is ");
int j;
for(j=i;j>=0;j++)
{
System.out.println("the binary no of" +a[i]);
}
}
}
